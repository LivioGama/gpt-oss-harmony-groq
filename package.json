{
  "name": "gpt-oss-harmony-groq",
  "version": "1.0.0",
  "description": "Transformers-based proxy for gpt-oss-20b with Groq API integration and harmony format support",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/server.ts",
    "start": "rm -f gpt-oss-harmony-groq.log && bun run src/server.ts",
    "build": "bun build src/server.ts --outdir dist",
    "test": "bun test"
  },
  "dependencies": {
    "@types/node": "^20.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  }
}
